<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <TextView
            android:text="TextView"
            android:layout_width="56dp"
            android:layout_height="29dp" android:id="@+id/_sundayTextView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="5dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="648dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="56dp"
            android:layout_height="29dp" android:id="@+id/_mondayTextView"
            android:layout_marginStart="5dp" app:layout_constraintStart_toEndOf="@+id/_sundayTextView"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="648dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="56dp"
            android:layout_height="29dp" android:id="@+id/_tuesdayTextView"
            android:layout_marginStart="5dp" app:layout_constraintStart_toEndOf="@+id/_mondayTextView"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="648dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="56dp"
            android:layout_height="29dp" android:id="@+id/_wednesdayTextView"
            android:layout_marginStart="5dp" app:layout_constraintStart_toEndOf="@+id/_tuesdayTextView"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="648dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="56dp"
            android:layout_height="29dp" android:id="@+id/_thursdayTextView"
            android:layout_marginStart="5dp" app:layout_constraintStart_toEndOf="@+id/_wednesdayTextView"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="648dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="56dp"
            android:layout_height="29dp" android:id="@+id/_fridayTextView"
            android:layout_marginStart="5dp" app:layout_constraintStart_toEndOf="@+id/_thursdayTextView"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="648dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="56dp"
            android:layout_height="29dp" android:id="@+id/_saturdayTextView"
            android:layout_marginEnd="5dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@+id/_fridayTextView" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="648dp"/>
    <TextView
            android:text="HOURS"
            android:layout_width="74dp"
            android:layout_height="28dp" android:id="@+id/_hoursTextView"
            android:layout_marginEnd="155dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.444"
            android:layout_marginStart="155dp" android:layout_marginTop="45dp"
            app:layout_constraintTop_toBottomOf="@+id/_secondDetailsImageView"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" tools:srcCompat="@tools:sample/avatars"
            android:id="@+id/_firstDetailsImageView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintEnd_toStartOf="@+id/_secondDetailsImageView" android:layout_marginEnd="32dp"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" tools:srcCompat="@tools:sample/avatars" android:id="@+id/_secondDetailsImageView"
            app:layout_constraintEnd_toStartOf="@+id/_thirdDetailsImageView"
            android:layout_marginEnd="32dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp" tools:srcCompat="@tools:sample/avatars" android:id="@+id/_thirdDetailsImageView"
            android:layout_marginEnd="32dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="18dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>