<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ypj2xqjc"
        tools:context=".activities.account.RegisterActivity" android:clickable="true">
    <ImageView
            android:layout_width="87dp"
            android:layout_height="64dp"
            android:id="@+id/imageView2"
            app:srcCompat="@drawable/user_profile"
            android:layout_marginEnd="160dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="84dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="146dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Email"
            android:ems="10"
            android:id="@+id/_username"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/_promptUser" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="88dp" android:layout_marginEnd="110dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/_password"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/_promptPassword" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="88dp" android:layout_marginEnd="110dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/_promptUser"
            android:layout_marginEnd="276dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" android:layout_marginTop="32dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="82dp"/>
    <TextView
            android:text="Password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/_promptPassword"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="89dp"
            android:layout_marginTop="33dp" app:layout_constraintTop_toBottomOf="@+id/_username"
            android:layout_marginEnd="259dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Register"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/_registerButton"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/_password" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="210dp" android:layout_marginEnd="113dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="379dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>