<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context="temple.edu.yelp_randomizer.fragments.RestaurantContentFragment">
    <WebView
            android:layout_width="408dp"
            android:layout_height="362dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="5dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/_contentRestaurantView" android:id="@+id/_restaurantWebView"
            app:layout_constraintVertical_bias="0.923"/>
    <temple.edu.yelp_randomizer.View.RestaurantView
            android:layout_width="411dp"
            android:layout_height="235dp"
            android:id="@+id/_contentRestaurantView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="19dp" app:layout_constraintTop_toBottomOf="@+id/_saveRestaurantButton"/>
    <ImageButton
            android:src="@android:drawable/ic_menu_save"
            android:layout_width="82dp"
            android:layout_height="71dp" android:id="@+id/_saveRestaurantButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="328dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="28dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>