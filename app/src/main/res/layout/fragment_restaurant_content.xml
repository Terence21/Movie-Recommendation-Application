<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context="temple.edu.random.fragments.RestaurantContentFragment">
    <temple.edu.random.View.RestaurantView
            android:layout_width="393dp"
            android:layout_height="206dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="4dp"
            android:id="@+id/_restaurantContentView" app:layout_constraintHorizontal_bias="0.0"/>
    <temple.edu.random.View.DetailsView android:layout_width="392dp" android:layout_height="395dp"
                                        android:id="@+id/_restaurantContentDetailsview"
                                        android:layout_marginTop="4dp"
                                        app:layout_constraintTop_toBottomOf="@+id/_restaurantContentView"
                                        app:layout_constraintStart_toStartOf="parent"
                                        android:layout_marginStart="2dp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="0.0"/>
    <com.google.android.material.button.MaterialButton android:layout_width="120dp" android:layout_height="39dp"
                                                       app:layout_constraintStart_toStartOf="parent"
                                                       android:layout_marginStart="132dp"
                                                       app:layout_constraintBottom_toBottomOf="parent"
                                                       android:layout_marginBottom="144dp"
                                                       android:text="Reviews"
                                                       android:id="@+id/_reviewsButton" android:layout_marginTop="25dp"
                                                       app:layout_constraintTop_toBottomOf="@+id/_restaurantContentDetailsview"
                                                       app:layout_constraintVertical_bias="0.363"
                                                       android:layout_marginEnd="141dp"
                                                       app:layout_constraintEnd_toEndOf="parent"
                                                       app:layout_constraintHorizontal_bias="1.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>